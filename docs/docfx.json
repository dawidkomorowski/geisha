{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Geisha.Engine/Geisha.Engine.csproj",
            "src/Geisha.Engine.Audio.CSCore/Geisha.Engine.Audio.CSCore.csproj",
            "src/Geisha.Engine.Input.Windows/Geisha.Engine.Input.Windows.csproj",
            "src/Geisha.Engine.Rendering.DirectX/Geisha.Engine.Rendering.DirectX.csproj",
            "src/Geisha.Engine.Windows/Geisha.Engine.Windows.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "tutorials/**.md",
          "tutorials/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Geisha Engine",
      "_enableSearch": true
    }
  }
}