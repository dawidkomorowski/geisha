    "RenderCollisionGeometry": true
  },
  "Rendering": {
    "EnableVSync": false,
    "ScreenWidth": 1920,
    "ScreenHeight": 1080,
    "ShowRenderingStatistics": true,
    "SortingLayersOrder": [
      "Default",
      "UI"
    ]
