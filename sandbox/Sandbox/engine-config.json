{
  "Audio": {
    "EnableSound": true,
    "Volume": 1.0
  },
  "Core": {
    "AssetsRootDirectoryPath": "Assets",
    "CustomGameLoopSteps": [ "SandboxSystem" ],
    "FixedUpdatesPerFrameLimit": 30,
    "FixedUpdatesPerSecond": 60,
    "LogLevel":  "Debug",
    "ShowAllEntitiesCount": true,
    "ShowRootEntitiesCount": true,
    "ShowFps": true,
    "ShowFrameTime": true,
    "ShowGameLoopStatistics": true,
    "ShowTotalFrames": true,
    "ShowTotalTime": true,
    //"StartUpScene": "Assets\\Scene\\Sandbox.geisha-scene"
    "StartUpSceneBehavior": "SandboxSceneBehavior"
  },
  "Physics": {
    "RenderCollisionGeometry": true
  },
  "Rendering": {
    "EnableVSync": false,
    "ScreenHeight": 900,
    "ScreenWidth": 1600,
    "ShowRenderingStatistics": true,
    "SortingLayersOrder": [
      "Default",
      "Box",
      "UI"
    ]
  }
}